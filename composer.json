{
    "name": "nitsan/ns-revolution-slider",
    "type": "typo3-cms-extension",
    "description": "World's most pupular revolution slider is now available in TYPO3 CMS too. Live-Demo: https://demo.t3planet.com/t3t-extensions/sliders/revolution-slider You can download PRO version for more-features & free-support at https://t3planet.com/typo3-slider-revolution-extension",
    "homepage": "https://extensions.typo3.org/extension/ns_revolution_slider/",
    "license": "GPL-2.0+",
    "keywords": [
      "TYPO3 CMS",
      "Revolution",
      "Slider"
    ],
    "support": {
      "issues": "https://github.com/nitsan-technologies/ns_revolution_slider/issues"
    },
    "require": {
         "typo3/cms-core": "^12.4 "
    },
    "autoload": {
        "psr-4": {
            "NITSAN\\NsRevolutionSlider\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/ns-revolution-slider": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor/",
        "bin-dir": ".Build/bin/"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "ns_revolution_slider"
        }
    }
}
